    "@tsconfig/node18": "18.2.0",
    "aws-cdk": "2.91.0",
    "aws-cdk-lib": "2.91.0",
    "prettier": "3.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "cdk-nag": "^2.27.100",
    "@umccr/elsa-data-aws-application": "file:../../workload-elsa-data-aws"
  }
}
