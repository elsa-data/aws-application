{
  "name": "manual",
  "version": "0.0.0",
  "private": true,
  "description": "A CDK pipeline for building/deploying a Elsa Data stack in the UMCCR environment",
  "scripts": {
    "dev-deploy": "cdk --app 'npx ts-node stacks.ts' deploy ElsaDataLocalDevTestStack",
    "dev-destroy": "cdk --app 'npx ts-node stacks.ts' destroy ElsaDataLocalDevTestStack",
    "agdemo-deploy": "cdk --app 'npx ts-node stacks.ts' deploy ElsaDataDemoAustralianGenomicsStack",
    "agdemo-destroy": "cdk --app 'npx ts-node stacks.ts' destroy ElsaDataDemoAustralianGenomicsStack"
  },
  "devDependencies": {
    "@tsconfig/node18": "2.0.0",
    "@types/node": "18.16.3",
    "aws-cdk": "2.77.0",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.77.0",
    "constructs": "10.2.13",
    "esbuild": "0.17.18",
    "source-map-support": "0.5.21"
  }
}
